{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "~7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "beberlei/assert": "^3.2",
        "firebase/php-jwt": "dev-master",
        "friendsofsymfony/rest-bundle": "2.5.0",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "~3.4.1",
        "knplabs/knp-paginator-bundle": "^4.0",
        "nelmio/api-doc-bundle": "~3.4",
        "nelmio/cors-bundle": "^1.5",
        "ramsey/uuid-doctrine": "~1.5",
        "sensio/framework-extra-bundle": "~5.4.1",
        "spatie/enum": "^2.3",
        "stof/doctrine-extensions-bundle": "~1.3",
        "symfony/apache-pack": "~1.0",
        "symfony/asset": "~4.3.4",
        "symfony/cache": "~4.3.4",
        "symfony/console": "~4.3.4",
        "symfony/flex": "~1.4.5",
        "symfony/form": "~4.3.4",
        "symfony/framework-bundle": "~4.3.4",
        "symfony/messenger": "~4.3.4",
        "symfony/monolog-bundle": "~3.4.0",
        "symfony/orm-pack": "~1.0.6",
        "symfony/security-bundle": "~4.3.4",
        "symfony/security-csrf": "~4.3.4",
        "symfony/serializer": "~4.3.4",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "~4.3.4",
        "symfony/twig-bundle": "~4.3.4",
        "symfony/validator": "~4.3.4",
        "symfony/yaml": "~4.3.4",
        "tetranz/select2entity-bundle": "^3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.8",
        "phpstan/phpstan": "~0.11.15",
        "phpstan/phpstan-doctrine": "~0.11.5",
        "phpstan/phpstan-symfony": "~0.11.6",
        "symfony/dotenv": "~4.3.4",
        "symfony/maker-bundle": "~1.13",
        "symfony/test-pack": "^1.0.6",
        "symfony/var-dumper": "~4.3.4",
        "symfony/web-server-bundle": "~4.3.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "~4.3.4"
        }
    }
}
